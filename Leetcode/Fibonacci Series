// Java solution 

class Solution {
    public int fib(int n) {
        if ( n < 2 ) 
            return n;
        
        int f = 0, s3=0;
        int s = 1;
        
        for ( int i = 1; i < n; i++ ) {
            s3 = f + s;
            f = s;
            s = s3;
        }
        
        return s3;
    }
}
